       <enumeration value="CAD"
                     appinfo="Computer-Assisted Design"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#CAD"/>
       <enumeration value="HSV"
                     appinfo="Hue, Saturation, and Value colour model"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#HSV"/>
       <enumeration value="JPEG"
                     appinfo="Joint Photographic Experts Group"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#JPEG"/>
       <enumeration value="MIDI"
                     appinfo="Musical Instrument Digital Interface. A standard for digital music representation."
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#MIDI"/>
       <enumeration value="MIME"
                     appinfo="Multipurpose Internet Mail Extension"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#MIME"/>
       <enumeration value="MF"
                     appinfo="Multiple-valued field"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#MF"/>
       <enumeration value="MPEG"
                     appinfo="Moving Picture Experts Group"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#MPEG"/>
       <enumeration value="PNG"
                     appinfo="Portable Network Graphics. A specification for representing two-dimensional images in files."
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#PNG"/>
       <enumeration value="RGB"
                     appinfo="Red, Green, and Blue colour model"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#RGB"/>
       <enumeration value="RURL"
                     appinfo="Relative Uniform Resource Locator"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#RURL"/>
       <enumeration value="SAI"
                     appinfo="Scene Access Interface"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#SAI"/>
       <enumeration value="SF"
                     appinfo="Single-valued field"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#SF"/>
       <enumeration value="UCS"
                     appinfo="Universal multiple-octet coded Character Set"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#UCS"/>
       <enumeration value="URI"
                     appinfo="Universal Resource Identifier"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#URI"/>
       <enumeration value="URL"
                     appinfo="Uniform Resource Locator"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#URL"/>
       <enumeration value="URN"
                     appinfo="Universal Resource Name"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#URN"/>
       <enumeration value="UTF-8"
                     appinfo="variable-length 8-bit Universal multiple-octet coded character set Transformation Format"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#UTF-8"/>
    </SimpleType>
    <SimpleType name="glossaryChoices"
                 baseType="xs:NMTOKEN"
                 appinfo="Glossary terms are defined in X3D Architecture: Extensible 3D (X3D), ISO/IEC 19775-1:2023, 3 Definitions, acronyms, and abbreviations.">
       <enumeration value="activate"
                     appinfo="cause a #sensorNode to generate an isActive #event"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Activate"/>
       <enumeration value="ancestor"
                     appinfo="#node which is an antecedent of another node in the #transformationHierarchy"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Ancestor"/>
       <enumeration value="AudioGraph"
                     alias="audio graph"
                     appinfo="structured collection of nodes that process audio inputs and outputs"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#AudioGraph"/>
       <enumeration value="author"
                     appinfo="person or agent that creates an #X3DFile"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Author"/>
       <enumeration value="AuthoringTool"
                     alias="authoring tool"
                     appinfo="see #generator"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#AuthoringTool"/>
       <enumeration value="avatar"
                     appinfo="abstract representation of the #user in an X3D #world"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Avatar"/>
       <enumeration value="bearing"
                     appinfo="straight line passing through the #pointer location in the direction of the pointer"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Bearing"/>
       <enumeration value="BindableNode"
                     alias="bindable node"
                     appinfo="#node that may have many #instances in a #sceneGraph but only one instance may be active at any instant of #time"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#BindableNode"/>
       <enumeration value="browser"
                     appinfo="computer program that interprets 3X3DFiles, presents their content to a user on a #displayDevice, and allows the user to interact with #worlds defined by X3D files by means of a user interface"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Browser"/>
       <enumeration value="BrowserExtension"
                     alias="browser extension"
                     appinfo="#nodes defined using the prototyping mechanism that are understood only by certain #browsers"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#BrowserExtension"/>
       <enumeration value="BuiltInNode"
                     alias="built-in node"
                     appinfo="#node of a #type explicitly defined in this document"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Built-InNode"/>
       <enumeration value="callback"
                     appinfo="function defined in a #scriptingLanguage to which #events are passed"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Callback"/>
       <enumeration value="child"
                     appinfo="instance of a #ChildrenNode"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Child"/>
       <enumeration value="ChildrenNode"
                     alias="children node"
                     appinfo="one of a set of #nodeTypes, instances of which can be collected in a group to share specific properties dependent on the type of the #GroupingNode"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#ChildrenNode"/>
       <enumeration value="ClientSystem"
                     alias="client system"
                     appinfo="computer system, attached to a #network, that relies on another computer (the server) for essential processing functions"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#ClientSystem"/>
       <enumeration value="CollisionProxy"
                     alias="collision proxy"
                     appinfo="#node used as a substitute for all of a Collision node's children during collision detection"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#CollisionProxy"/>
       <enumeration value="ColourModel"
                     alias="colour model color model"
                     appinfo="characterization of a colour space in terms of explicit parameters"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#ColourModel"/>
       <enumeration value="culling"
                     appinfo="process of identifying #objects or parts of objects which do not need to be processed further by the #browser in order to produce the desired view of a #world"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Culling"/>
       <enumeration value="descendant"
                     appinfo="#node which descends from another node in the #transformationHierarchy (a #ChildrenNode)"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Descendant"/>
       <enumeration value="DisplayDevice"
                     alias="display device"
                     appinfo="graphics device on which X3D #worlds may be rendered"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#DisplayDevice"/>
       <enumeration value="DragSensor"
                     alias="drag sensor"
                     appinfo="#pointingDeviceSensor that causes #events to be generated in response to sensor-dependent pointer motions"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#DragSensor"/>
       <enumeration value="EnvironmentalSensor"
                     alias="environmental sensor"
                     appinfo="sensor #node that generates #events based on the location of the viewpoint in the #world or in relation to #objects in the world"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#EnvironmentalSensor"/>
       <enumeration value="event"
                     appinfo="message sent from one #node to another as defined by a #ROUTE"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Event"/>
       <enumeration value="EventCascade"
                     alias="event cascade"
                     appinfo="sequence of @events initiated by a script or sensor event and propagated from #node to node along one or more #routes all of which are considered to have occurred simultaneously"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#EventCascade"/>
       <enumeration value="ExecutionModel"
                     alias="execution model"
                     appinfo="rules governing how #events are processed by #browsers and scripts"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#ExecutionModel"/>
       <enumeration value="ExternaPrototype"
                     alias="external prototype"
                     appinfo="#prototype defined in an external file and referenced by a #URL"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#ExternaPrototype"/>
       <enumeration value="field"
                     appinfo="property or attribute of a #node"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Field"/>
       <enumeration value="FieldName"
                     alias="field name"
                     appinfo="identifier of a #field"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#FieldName"/>
       <enumeration value="frame"
                     appinfo="single rendering of a #world on a #displayDevice or a single time-step in a simulation"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Frame"/>
       <enumeration value="generator"
                     appinfo="computer program which creates #X3DFiles"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Generator"/>
       <enumeration value="GeometricPropertyNode"
                     alias="geometric property node"
                     appinfo="#node defining the properties of a specific geometry node"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#GeometricPropertyNode"/>
       <enumeration value="GeometryNode"
                     alias="geometry node"
                     appinfo="#node containing mathematical descriptions of points, lines, surfaces, text strings and solids"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#GeometryNode"/>
       <enumeration value="grab"
                     appinfo="receive #events from activated #pointingDevices"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Grab"/>
       <enumeration value="GroupingNode"
                     alias="grouping node"
                     appinfo="one of a set of #nodeTypes which include a list of nodes, referred to as its #ChildrenNodes"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#GroupingNode"/>
       <enumeration value="HostApplication"
                     alias="host application"
                     appinfo="client application with which the #browser communicates using the SAI"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#HostApplication"/>
       <enumeration value="image"
                     appinfo="two-dimensional (2D) rectangular array of pixel values"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Image"/>
       <enumeration value="immersive"
                     appinfo="creating the illusion of being inside a computer-generated scene"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Immersive"/>
       <enumeration value="Inlining"
                     alias="in-lining"
                     appinfo="mechanism by which one #X3DFile is hierarchically included in another"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#In-lining"/>
       <enumeration value="instance"
                     appinfo="the #node created by an #instantiation"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Instance"/>
       <enumeration value="instantiation"
                     appinfo="the creation of a #node based on its #nodeType"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Instantiation"/>
       <enumeration value="InterpolatorNode"
                     alias="interpolator node"
                     appinfo="#node that defines a piece-wise or smoothly continuous interpolation"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#InterpolatorNode"/>
       <enumeration value="intranet"
                     appinfo="private #network that uses the same protocols and standards as the Internet"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Intranet"/>
       <enumeration value="LevelOfDetail"
                     alias="level of detail"
                     appinfo="amount of detail or complexity which is displayed at any particular #time for any particular #object"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#LevelOfDetail"/>
       <enumeration value="LineTerminator"
                     alias="line terminator"
                     appinfo="linefeed character (0x0A) and/or carriage return character (0x0D)"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#LineTerminator"/>
       <enumeration value="loop"
                     appinfo="sequence of #events which would result in a specific event generator sending more than one event with the same #timestamp"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Loop"/>
       <enumeration value="multimedia"
                     appinfo="integrated presentation, typically on a computer, of content of various types, such as computer graphics, audio, and videon"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Multimedia"/>
       <enumeration value="network"
                     appinfo="set of interconnected computers"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Network"/>
       <enumeration value="node"
                     appinfo="fundamental component of a #sceneGraph that defines model information, such as graph structure, geometry, animation, interaction, object metadata, rendering, or display"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Node"/>
       <enumeration value="nodeType"
                     alias="node type"
                     appinfo="characteristic of each node that describes, in general, its particular semantics"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#NodeType"/>
       <enumeration value="object"
                     appinfo="collection of data and procedures, packaged according to the rules and syntax defined in this document. NOTE This term is usually synonymous with #node."
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Object"/>
       <enumeration value="orderOfPreference"
                     alias="order of preference"
                     appinfo="order (specified by the user) in which a list of #field values is processed by the #browser"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#OrderOfPreference"/>
       <enumeration value="panorama"
                     appinfo="background texture that is placed behind all geometry in the scene and in front of the ground and sky"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Panorama"/>
       <enumeration value="parent"
                     appinfo="#node which is an instance of a #GroupingNode"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Parent"/>
       <enumeration value="pixel"
                     appinfo="one element of an #image specified as a matrix of colour elements"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Pixel"/>
       <enumeration value="pointer"
                     appinfo="location and direction in the #virtualWorld defined by the #pointingDevice with which the #user is currently interacting with the virtual world"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Pointer"/>
       <enumeration value="pointingDevice"
                     alias="pointing device"
                     appinfo="hardware device connected to the #user's computer by which the user directly controls the location and direction of the #pointer"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#PointingDevice"/>
       <enumeration value="pointingDeviceSensor"
                     alias="pointing device sensor"
                     appinfo="sensor #node that generates #events based on #user actions, such as #pointingDevice motions or button activations"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#PointingDeviceSensor"/>
       <enumeration value="polyline"
                     appinfo="piecewise linear curve"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Polyline"/>
       <enumeration value="profile"
                     appinfo="named collection of criteria for functionality and conformance that defines an implementable subset of a standard"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Profile"/>
       <enumeration value="prototype"
                     appinfo="definition of a new #nodeType in terms of the nodes defined in this document"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Prototype"/>
       <enumeration value="prototyping"
                     appinfo="mechanism for extending the set of #nodeTypes from within a #X3DFile"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Prototyping"/>
       <enumeration value="ROUTE"
                     appinfo="connection between a #node generating an #event and a node receiving the event"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#ROUTE"/>
       <enumeration value="sceneGraph"
                     alias="scene graph"
                     appinfo="ordered set of #nodes and #statements"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#SceneGraph"/>
       <enumeration value="script"
                     appinfo="set of procedural functions normally executed as part of an #eventCascade"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Script"/>
       <enumeration value="scripting"
                     appinfo="process of creating or referring to a #script"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Scripting"/>
       <enumeration value="sensorNode"
                     alias="sensor node"
                     appinfo="#node that enables the #user to interact with the #world in the scene graph hierarchy"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#SensorNode"/>
       <enumeration value="separatorCharacter"
                     alias="separator character"
                     appinfo="#UTF-8 character used to separate syntactical entities in an #X3DFile"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#SeparatorCharacter"/>
       <enumeration value="sibling"
                     appinfo="#node which shares a #parent with other nodes"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#Sibling"/>
       <enumeration value="simulationTick"
                     alias="simulation tick"
                     appinfo="smallest time unit capable of being identified in a digital simulation of analog time"
                     documentation="https://www.web3d.org/specifications/X3Dv4Draft/ISO-IEC19775-1v4-IS.proof/Part01/glossary.html#SimulationTick"/>
       <enumeration value="slerp"
                     appinfo="spherical linear interpolation for animating 3D rotation values"
